				</b:FormGroup>
				<b:FormGroup size="SMALL">
					<b:FormLabel text="Command Name:" />
					<b:Input ui:field="mCommandName" type="TEXT" />
				</b:FormGroup>

				<b:FormGroup size="SMALL">
					<b:FormLabel text="Button Type:" />
