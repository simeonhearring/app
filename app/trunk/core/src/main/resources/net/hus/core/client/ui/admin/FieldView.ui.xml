<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:b.select="urn:import:org.gwtbootstrap3.extras.select.client.ui" xmlns:b.toggle="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui">
	<ui:with field="msg" type="net.hus.core.client.resource.Messages" />
	<ui:style>

	</ui:style>
	<b:Row>
		<b:Column size="SM_3">
			<b:Panel type="PRIMARY">
				<b:PanelHeader>
					<b:Heading size="H4" text="Select Field" />
				</b:PanelHeader>
				<b:PanelBody>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Name:" />
						<b.select:Select ui:field="mFields" liveSearch="true" visible="true" />
					</b:FormGroup>
				</b:PanelBody>
			</b:Panel>
			<b:Panel type="PRIMARY">
				<b:PanelHeader>
					<b:Heading size="H4" text="Create New Field" />
				</b:PanelHeader>
				<b:PanelBody>
					<b:FormGroup size="SMALL" ui:field="mAddNameGrp">
						<b:FormLabel text="Name:" />
						<b:Input ui:field="mAddName" placeholder="Enter Field Name" allowBlank="false" validateOnBlur="true" type="TEXT" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Type:" />
						<b:ListBox ui:field="mAddType" />
					</b:FormGroup>
					<b:Tooltip text="Add New">
						<b:Icon ui:field="mAdd0" type="SAVE" paddingLeft="15" addStyleNames="pull-right" />
					</b:Tooltip>
				</b:PanelBody>
			</b:Panel>
		</b:Column>
		<b:Column size="SM_9">
			<b:TabPanel tabPosition="TOP">
				<b:NavTabs>
					<b:TabListItem dataTarget="#tab4" text="Name" active="true" ui:field="mNameTab" />
					<b:TabListItem dataTarget="#tab5" text="Display" />
					<b:TabListItem dataTarget="#tab6" text="Date" ui:field="mDateTab" />
					<b:TabListItem dataTarget="#tab7" text="Array" ui:field="mArrayTab" />
					<b:TabListItem dataTarget="#tab8" text="Lookup" ui:field="mLookupTab" />
				</b:NavTabs>
				<b:TabContent>
					<b:TabPane fade="true" in="true" active="true" b:id="tab4">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Field Name" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="ID:" />
											<b.html:Paragraph ui:field="mId" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Name:" />
											<b.html:Paragraph ui:field="mName" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Type:" />
											<b.html:Paragraph ui:field="mType" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tab5">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Field Display" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Long:" />
											<b:Input ui:field="mLong" type="TEXT" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Short:" />
											<b:Input ui:field="mShort" type="TEXT" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave0" type="SAVE" paddingLeft="10" addStyleNames="pull-right" />
								</b:Tooltip>
								<b:Tooltip text="Refresh">
									<b:Icon ui:field="mRefresh0" type="REFRESH" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tab6">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Field Date" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Storage format:" />
											<b:Input ui:field="mStorageFormat" type="TEXT" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Example:" />
											<b.html:Paragraph ui:field="mStorageFormatEx" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave1" type="SAVE" paddingLeft="10" addStyleNames="pull-right" />
								</b:Tooltip>
								<b:Tooltip text="Refresh">
									<b:Icon ui:field="mRefresh1" type="REFRESH" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tab7">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Field Array" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Header Size:" />
											<b:ListBox ui:field="mHeadSize" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Show Bottom Icons At Row:" />
											<b:Input ui:field="mBottomRow" type="NUMBER" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Row>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Alternate Color:&nbsp;" />
											<b.toggle:ToggleSwitch ui:field="mAltColor" size="MINI" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Alternate Color Even:" />
											<b:Input ui:field="mAltEven" type="COLOR" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Alternate Color Odd:" />
											<b:Input ui:field="mAltOdd" type="COLOR" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Row>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Size:&nbsp;" />
											<b.html:Span ui:field="mSize" />
											<b:Tooltip text="Add">
												<b:Icon ui:field="mPlus" type="PLUS" paddingLeft="10" paddingRight="10" />
											</b:Tooltip>
											<b:Tooltip text="Remove">
												<b:Icon ui:field="mMinus" type="MINUS" />
											</b:Tooltip>
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Headers:" />
											<b.gwt:FlowPanel ui:field="mArrayLabel" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL" ui:field="mTableFieldsGrp">
											<b:FormLabel text="Table Fields:" />
											<b.gwt:FlowPanel ui:field="mTableFields" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL" ui:field="mCTypesGrp">
											<b:FormLabel text="Type:" />
											<b.gwt:FlowPanel ui:field="mCTypes" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave2" type="SAVE" paddingLeft="10" addStyleNames="pull-right" />
								</b:Tooltip>
								<b:Tooltip text="Refresh">
									<b:Icon ui:field="mRefresh2" type="REFRESH" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tab8">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Field Lookup" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Location:" />
											<b:ListBox ui:field="mLookupLocation" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Parameters:" />
											<b.html:Paragraph ui:field="mLookupGroupText" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Row>
									<b:Column size="SM_12">
										<b.gwt:FlowPanel ui:field="mLookupGroup" styleName="core_columns" />
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave3" type="SAVE" paddingLeft="10" addStyleNames="pull-right" />
								</b:Tooltip>
								<b:Tooltip text="Refresh">
									<b:Icon ui:field="mRefresh3" type="REFRESH" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
				</b:TabContent>
				<b.html:ClearFix />
			</b:TabPanel>
		</b:Column>
	</b:Row>
</ui:UiBinder>