<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b.select="urn:import:org.gwtbootstrap3.extras.select.client.ui">
	<ui:style>
	.row {
		
	}
	
	.sidenav {
		padding-top: 20px;
		background-color: #f1f1f1;
		height: 100%;
	}
	</ui:style>

	<b:Container fluid="true" addStyleNames="text-Center">
		<b:Row ui:field="mRow0" addStyleNames="content {style.row}">
			<b:Column ui:field="mL01" size="SM_2" addStyleNames="{style.sidenav}"></b:Column>
			<b:Column ui:field="mC01" size="SM_8" addStyleNames="text-center"></b:Column>
			<b:Column ui:field="mR01" size="SM_2" addStyleNames="{style.sidenav}"></b:Column>
		</b:Row>
		<b:Row ui:field="mRow1" addStyleNames="content {style.row}">
			<b:Column size="SM_1"></b:Column>
			<b:Column size="SM_10" addStyleNames="text-left">
				<b:Row>
					<b:Column size="SM_3">
						<b:FormGroup>
							<b:FormLabel>Field</b:FormLabel>
							<b.select:Select ui:field="mSelect" liveSearch="true" visible="true" />
						</b:FormGroup>
					</b:Column>
					<b:Column size="SM_9">
					</b:Column>
				</b:Row>
			</b:Column>
			<b:Column size="SM_1"></b:Column>
		</b:Row>
	</b:Container>
</ui:UiBinder>