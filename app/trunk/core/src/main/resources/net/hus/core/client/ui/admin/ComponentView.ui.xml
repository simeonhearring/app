<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:b.select="urn:import:org.gwtbootstrap3.extras.select.client.ui" xmlns:b.toggle="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
	xmlns:c="urn:import:net.hus.core.client.ui.components">
	<ui:with field="msg" type="net.hus.core.client.resource.Messages" />
	<ui:style>
	
	</ui:style>
	<b:Row>
		<b:Column size="SM_3">
			<b:Panel type="PRIMARY">
				<b:PanelHeader>
					<b:Heading size="H4" text="{msg.admin_Heading_editPage}" />
				</b:PanelHeader>
				<b:PanelBody>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Select Page:" />
						<b.select:Select ui:field="mPages" liveSearch="true" visible="true" />
					</b:FormGroup>
				</b:PanelBody>
			</b:Panel>
			<b:Panel type="PRIMARY">
				<b:PanelHeader>
					<b:Heading size="H4" text="{msg.admin_Heading_createPage}" />
				</b:PanelHeader>
				<b:PanelBody>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Name:" />
						<b:Input ui:field="mAddPage" placeholder="Enter Page Name" allowBlank="false" validateOnBlur="true" type="TEXT" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="FVT:" />
						<b:ListBox ui:field="mAddFvt" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="FGG:" />
						<b:ListBox ui:field="mAddFgg" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Page layout:" />
						<b:ListBox ui:field="mAddPageName" />
					</b:FormGroup>
					<b:Tooltip text="Add New">
						<b:Icon ui:field="mAdd0" type="SAVE" addStyleNames="pull-right" />
					</b:Tooltip>
				</b:PanelBody>
			</b:Panel>
		</b:Column>
		<b:Column size="SM_9">
			<b:TabPanel tabPosition="TOP">
				<b:NavTabs>
					<b:TabListItem dataTarget="#tabc4" text="Name" active="true" />
					<b:TabListItem dataTarget="#tabc5" text="TKG" />
					<b:TabListItem dataTarget="#tabc6" text="Components" />
				</b:NavTabs>
				<b:TabContent>
					<b:TabPane fade="true" in="true" active="true" b:id="tabc4">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Page name" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="ID:" />
											<b.html:Paragraph ui:field="mName" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Name:" />
											<b:Input ui:field="mDisplay" type="TEXT" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave0" type="SAVE" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tabc5">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Page Field TKG" />
							</b:PanelHeader>
							<b:PanelBody>
								<b.html:Paragraph ui:field="mPageNameF" />
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="FVT:" />
											<b:ListBox ui:field="mFvt" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="FGG:" />
											<b:ListBox ui:field="mFgg" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Page layout:" />
											<b:ListBox ui:field="mPageName" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave1" type="SAVE" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tabc6">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Page Components" />
							</b:PanelHeader>
							<b:PanelBody>
								<b.html:Paragraph ui:field="mPageNameC" />
								<b:PanelGroup b:id="accordion">
									<b:Panel type="PRIMARY">
										<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion2" dataTarget="#collapseOne">
											<b:Heading size="H5" text="Hierarchy" />
										</b:PanelHeader>
										<b:PanelCollapse b:id="collapseOne">
											<b:PanelBody>
												<b.gwt:FlowPanel ui:field="mTree" />
											</b:PanelBody>
										</b:PanelCollapse>
									</b:Panel>
									<b:Panel type="PRIMARY" ui:field="mDetailPanel" visible="false">
										<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion2" dataTarget="#collapseTwo">
											<b:Heading size="H5" text="Detail" />
										</b:PanelHeader>
										<b:PanelCollapse b:id="collapseTwo">
											<b:PanelBody>
												<b.gwt:FlowPanel ui:field="mAdd">
													<b:Row>
														<b:Column size="SM_2">
															<b.html:Span styleName="nowrap" text="Add Component ..." />
														</b:Column>
														<b:Column size="SM_3">
															<b:FormGroup size="SMALL">
																<b:ListBox ui:field="mComponents" />
															</b:FormGroup>
														</b:Column>
														<b:Column size="SM_2" ui:field="mAddToCol">
															<b.html:Span text="...to&nbsp;" />
															<b.html:Span ui:field="mPageNameA" />
															<b.html:Span text="&nbsp;" />
															<b:Tooltip text="Add To">
																<b:Icon ui:field="mAdd1" type="PLUS" />
															</b:Tooltip>
														</b:Column>
														<b:Column size="SM_2">
															<b.html:Span text="...&nbsp;to&nbsp;" />
															<b.html:Span ui:field="mRoot" text="Root&nbsp;" />
															<b:Tooltip text="Add To">
																<b:Icon ui:field="mAdd2" type="PLUS" />
															</b:Tooltip>
														</b:Column>
													</b:Row>
													<b.html:Hr />
												</b.gwt:FlowPanel>
												<b.gwt:FlowPanel ui:field="mDetail" />
											</b:PanelBody>
										</b:PanelCollapse>
									</b:Panel>
								</b:PanelGroup>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
				</b:TabContent>
				<b.html:ClearFix />
			</b:TabPanel>
		</b:Column>
	</b:Row>
</ui:UiBinder>