<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:b.select="urn:import:org.gwtbootstrap3.extras.select.client.ui" xmlns:b.toggle="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
	xmlns:c="urn:import:net.hus.core.client.ui.components">
	<ui:with field="msg" type="net.hus.core.client.resource.Messages" />
	<ui:style>

	</ui:style>
	<b:Row>
		<b:Column size="SM_3">
			<b:Panel type="PRIMARY">
				<b:PanelHeader>
					<b:Heading size="H4" text="Edit Profile" />
				</b:PanelHeader>
				<b:PanelBody>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Select Profile:" />
						<b.select:Select ui:field="mProfiles" liveSearch="true" visible="true" />
					</b:FormGroup>
				</b:PanelBody>
			</b:Panel>
			<b:Panel type="PRIMARY">
				<b:PanelHeader>
					<b:Heading size="H4" text="Create New Profile" />
				</b:PanelHeader>
				<b:PanelBody>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Name:" />
						<b:Input ui:field="mAddUserName" placeholder="Enter user name" allowBlank="false" validateOnBlur="true" type="TEXT" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="First:" />
						<b:Input ui:field="mAddFirst" placeholder="Enter first name" allowBlank="false" validateOnBlur="true" type="TEXT" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Last:" />
						<b:Input ui:field="mAddLast" placeholder="Enter last name" allowBlank="false" validateOnBlur="true" type="TEXT" />
					</b:FormGroup>
					<b:FormGroup size="SMALL">
						<b:FormLabel text="Landing page:" />
						<b:ListBox ui:field="mAddPage" />
					</b:FormGroup>
					<b:Tooltip text="Add New">
						<b:Icon ui:field="mAdd0" type="SAVE" addStyleNames="pull-right" />
					</b:Tooltip>
				</b:PanelBody>
			</b:Panel>
		</b:Column>
		<b:Column size="SM_9">
			<b:TabPanel tabPosition="TOP">
				<b:NavTabs>
					<b:TabListItem dataTarget="#tabp4" text="Name" active="true" />
					<b:TabListItem dataTarget="#tabp5" text="Detail" />
				</b:NavTabs>
				<b:TabContent>
					<b:TabPane fade="true" in="true" active="true" b:id="tabp4">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Profile Info" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Created:" />
											<b.html:Paragraph ui:field="mCreated" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_3">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Updated:" />
											<b.html:Paragraph ui:field="mUpdated" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_2">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="ID:" />
											<b.html:Paragraph ui:field="mId" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Name:" />
											<b.html:Paragraph ui:field="mName" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tabp5">
						<b:Panel type="PRIMARY">
							<b:PanelHeader>
								<b:Heading size="H4" text="Profile Detail" />
							</b:PanelHeader>
							<b:PanelBody>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="First:" />
											<b:Input ui:field="mFirst" type="TEXT" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Middle:" />
											<b:Input ui:field="mMiddle" type="TEXT" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Last:" />
											<b:Input ui:field="mLast" type="TEXT" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="User name:" />
											<b.html:Paragraph ui:field="mUserName" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Password:" />
											<b:Input ui:field="mPassword" type="TEXT" />
										</b:FormGroup>
									</b:Column>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Landing page:" />
											<b:ListBox ui:field="mPage" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Row>
									<b:Column size="SM_4">
										<b:FormGroup size="SMALL">
											<b:FormLabel text="Theme:" />
											<b:ListBox ui:field="mCss" />
										</b:FormGroup>
									</b:Column>
								</b:Row>
								<b:Tooltip text="Save">
									<b:Icon ui:field="mSave0" type="SAVE" addStyleNames="pull-right" />
								</b:Tooltip>
							</b:PanelBody>
						</b:Panel>
					</b:TabPane>
				</b:TabContent>
				<b.html:ClearFix />
			</b:TabPanel>
		</b:Column>
	</b:Row>
</ui:UiBinder>