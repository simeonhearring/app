<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.row {
		height: 450px
	}
	
	.sidenav {
		padding-top: 20px;
		background-color: #f1f1f1;
		height: 100%;
	}
	</ui:style>

	<b:Container fluid="true" addStyleNames="text-Center">
		<b:Row ui:field="mRow" addStyleNames="content {style.row}">
			<b:Column ui:field="mWebL01" size="SM_2" addStyleNames="{style.sidenav}"></b:Column>
			<b:Column ui:field="mWebC01" size="SM_8" addStyleNames="text-left"></b:Column>
			<b:Column ui:field="mWebR01" size="SM_2" addStyleNames="{style.sidenav}"></b:Column>
		</b:Row>
	</b:Container>
</ui:UiBinder>