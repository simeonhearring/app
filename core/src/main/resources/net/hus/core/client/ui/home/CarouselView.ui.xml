<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:import field="org.gwtbootstrap3.client.ui.constants.IconType.*" />
	<ui:style>
	
	</ui:style>
	<b:Carousel ui:field="mCarousel" b:id="carousel-captions">
		<b:CarouselIndicators ui:field="mIndicators">
			<b:CarouselIndicator ui:field="mInd0" dataTarget="#carousel-captions" dataSlideTo="0" active="true" />
			<b:CarouselIndicator ui:field="mInd1" dataTarget="#carousel-captions" dataSlideTo="1" />
			<b:CarouselIndicator ui:field="mInd2" dataTarget="#carousel-captions" dataSlideTo="2" />
			<b:CarouselIndicator ui:field="mInd3" dataTarget="#carousel-captions" dataSlideTo="3" />
		</b:CarouselIndicators>
		<b:CarouselInner>
			<b:CarouselSlide active="true">
				<b:Image ui:field="mImg1" />
				<b:CarouselCaption>
					<b:Heading ui:field="mHead1" size="H1" />
					<b.html:Paragraph ui:field="mPar1" />
				</b:CarouselCaption>
			</b:CarouselSlide>
			<b:CarouselSlide>
				<b:Image ui:field="mImg2" />
				<b:CarouselCaption>
					<b:Heading ui:field="mHead2" size="H1" />
					<b.html:Paragraph ui:field="mPar2" />
				</b:CarouselCaption>
			</b:CarouselSlide>
			<b:CarouselSlide>
				<b:Image ui:field="mImg3" />
				<b:CarouselCaption>
					<b:Heading ui:field="mHead3" size="H1" />
					<b.html:Paragraph ui:field="mPar3" />
				</b:CarouselCaption>
			</b:CarouselSlide>
			<b:CarouselSlide>
				<b:Image ui:field="mImg4" />
				<b:CarouselCaption>
					<b:Heading ui:field="mHead4" size="H1" />
					<b.html:Paragraph ui:field="mPar4" />
				</b:CarouselCaption>
			</b:CarouselSlide>
		</b:CarouselInner>
		<b:CarouselControl ui:field="mLeft" dataTarget="#carousel-captions" prev="true" iconType="CHEVRON_LEFT" />
		<b:CarouselControl ui:field="mRight" dataTarget="#carousel-captions" next="true" iconType="CHEVRON_RIGHT" />
	</b:Carousel>
</ui:UiBinder>