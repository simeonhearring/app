<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:b.select="urn:import:org.gwtbootstrap3.extras.select.client.ui" xmlns:b.toggle="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
	xmlns:c="urn:import:net.hus.core.client.ui.components">
	<ui:style>
	
	</ui:style>
	<b:Row>
		<b:Column size="SM_3">
			<b:Well size="LARGE">
				<b:Legend text="EDIT PAGE" />
				<b:FormGroup>
					<b:FormLabel text="Select Page:" />
					<b.select:Select ui:field="mPages" liveSearch="true" visible="true" />
				</b:FormGroup>
			</b:Well>
			<b:Well size="LARGE">
				<b:Legend text="CREATE NEW PAGE" />
				<b:FormGroup>
					<b:FormLabel text="Name:" />
					<b:Input ui:field="mAddPage" placeholder="Enter Page Name" allowBlank="false" validateOnBlur="true" type="TEXT" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="FVT:" />
					<b:ListBox ui:field="mAddFvt" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="FGG:" />
					<b:ListBox ui:field="mAddFgg" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="Page layout:" />
					<b:ListBox ui:field="mAddPageName" />
				</b:FormGroup>
				<b:Tooltip text="Add New">
					<b:Icon ui:field="mAdd0" type="SAVE" addStyleNames="pull-right" />
				</b:Tooltip>
			</b:Well>
		</b:Column>
		<b:Column size="SM_9">
			<b:TabPanel tabPosition="LEFT">
				<b:NavTabs>
					<b:TabListItem dataTarget="#tabc4" text="Name" active="true" />
					<b:TabListItem dataTarget="#tabc5" text="Page" />
					<b:TabListItem dataTarget="#tabc6" text="Components" />
				</b:NavTabs>
				<b:TabContent>
					<b:TabPane fade="true" in="true" active="true" b:id="tabc4">
						<b:Well size="LARGE">
							<b:Legend text="PAGE NAME" />
							<b:FormGroup>
								<b:FormLabel text="Name:" />
								<b.html:Paragraph ui:field="mName" />
							</b:FormGroup>
							<b:FormGroup>
								<b:FormLabel text="Display:" />
								<b:Input ui:field="mDisplay" type="TEXT" />
							</b:FormGroup>
							<b:Tooltip text="Save">
								<b:Icon ui:field="mSave0" type="SAVE" addStyleNames="pull-right" />
							</b:Tooltip>
						</b:Well>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tabc5">
						<b:Well size="LARGE">
							<b:Legend text="FIELD TKG" />
							<b.html:Paragraph ui:field="mPageNameF" />
							<b:FormGroup>
								<b:FormLabel text="FVT:" />
								<b:ListBox ui:field="mFvt" />
							</b:FormGroup>
							<b:FormGroup>
								<b:FormLabel text="FGG:" />
								<b:ListBox ui:field="mFgg" />
							</b:FormGroup>
							<b:FormGroup>
								<b:FormLabel text="Page layout:" />
								<b:ListBox ui:field="mPageName" />
							</b:FormGroup>
							<b:Tooltip text="Save">
								<b:Icon ui:field="mSave1" type="SAVE" addStyleNames="pull-right" />
							</b:Tooltip>
						</b:Well>
					</b:TabPane>
					<b:TabPane fade="true" b:id="tabc6">
						<b:Well size="LARGE">
							<b:Legend text="COMPONENTS" />
							<b.html:Paragraph ui:field="mPageNameC" />
							<b:PanelGroup b:id="accordion">
								<b:Panel>
									<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion2" dataTarget="#collapseOne">
										<b:Heading size="H5" text="IN PAGE" />
									</b:PanelHeader>
									<b:PanelCollapse b:id="collapseOne">
										<b:PanelBody>
											<b.gwt:FlowPanel ui:field="mTree" />
										</b:PanelBody>
									</b:PanelCollapse>
								</b:Panel>
								<b:Panel>
									<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion2" dataTarget="#collapseTwo">
										<b:Heading size="H5" text="DETAIL" />
									</b:PanelHeader>
									<b:PanelCollapse b:id="collapseTwo">
										<b:PanelBody>
											<b.html:Paragraph ui:field="mComponentName" />
										</b:PanelBody>
									</b:PanelCollapse>
								</b:Panel>
							</b:PanelGroup>
						</b:Well>
					</b:TabPane>
				</b:TabContent>
				<b.html:ClearFix />
			</b:TabPanel>
		</b:Column>
	</b:Row>
</ui:UiBinder>