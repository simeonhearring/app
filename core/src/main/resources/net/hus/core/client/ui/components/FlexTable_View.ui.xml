<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt">
	<ui:style>
	.row {
		height: 450px
	}
	
	.sidenav {
		padding-top: 20px;
		background-color: #f1f1f1;
		height: 100%;
	}
	</ui:style>

	<b.gwt:FlowPanel>
		<b:Tooltip text="Add new row">
			<b:Icon ui:field="mAdd0" type="PLUS" paddingRight="10" />
		</b:Tooltip>
		<b:Tooltip text="Save">
			<b:Icon ui:field="mSave0" type="SAVE" />
		</b:Tooltip>
		<g:FlexTable ui:field="mComponent" />
		<b:Tooltip text="Add new row">
			<b:Icon ui:field="mAdd1" type="PLUS" paddingRight="10" visible="false" />
		</b:Tooltip>
		<b:Tooltip text="Save">
			<b:Icon ui:field="mSave1" type="SAVE" visible="false" />
		</b:Tooltip>
	</b.gwt:FlowPanel>
</ui:UiBinder>